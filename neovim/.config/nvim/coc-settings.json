{
    "eslint.run": "onType",
    "prettier.eslintIntegration": true,
    "explorer.icon.enableNerdfont": true,
    "explorer.file.showHiddenFiles": true,
    "explorer.file.reveal.auto": true,
    "explorer.focus": true,
    "diagnostic.enableSign": true,
    "diagnostic.infoSign": "üîµ",
    "diagnostic.hintSign": "‚ö™Ô∏è",
    "diagnostic.warningSign": "‚ö†Ô∏è",
    "diagnostic.errorSign": "‚ùå",
    "git.addedSign.text": "‚îÉ",
    "git.removedSign.text": "‚îÉ",
    "git.changedSign.text": "‚îÉ",
    "git.changeRemovedSign.text": "‚îÉ",
    // "git.changedSign.hlGroup": "IncSearch",         // This hack should only be needed with the OceanicNext colorscheme
    "git.blameUseRealTime": true,
    "list.source.symbols.excludes": ["*/node_modules/*"],
